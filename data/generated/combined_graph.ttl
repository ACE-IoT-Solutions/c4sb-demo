@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix core: <https://w3id.org/rec/core/> .
@prefix ex: <http://example.com/building#> .
@prefix ex1: <http://example.com/mybuilding#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix props: <https://w3id.org/rec/props/> .
@prefix quantitykind: <http://qudt.org/vocab/quantitykind/> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ref: <https://w3id.org/rec/ref/> .
@prefix s223: <http://data.ashrae.org/standard223#> .
@prefix unit: <http://qudt.org/vocab/unit/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:office_space_A a core:Office ;
    rdfs:label "Office Space A" ;
    core:isPartOfSpace ex:room_101 .

ex1:RTU-1_DischargeAirTemperatureSensor a s223:Sensor ;
    s223:hasQuantityKind quantitykind:Temperature ;
    s223:hasTag "discharge",
        "sensor",
        "temperature" ;
    s223:hasUnit unit:DEG_C ;
    s223:isPointOf ex1:RTU-1 .

ex1:RTU-1_DischargeAirTemperatureSetpoint a s223:Parameter ;
    s223:hasQuantityKind quantitykind:Temperature ;
    s223:hasTag "discharge",
        "setpoint",
        "temperature" ;
    s223:hasUnit unit:DEG_C ;
    s223:isPointOf ex1:RTU-1 .

ex1:RTU-1_FilterStatus a s223:State ;
    s223:hasTag "filter",
        "status" ;
    s223:isPointOf ex1:RTU-1 .

ex1:RTU-1_MixedAirTemperatureSensor a s223:Sensor ;
    s223:hasQuantityKind quantitykind:Temperature ;
    s223:hasTag "mixed",
        "sensor",
        "temperature" ;
    s223:hasUnit unit:DEG_C ;
    s223:isPointOf ex1:RTU-1 .

ex1:RTU-1_ReturnAirTemperatureSensor a s223:Sensor ;
    s223:hasQuantityKind quantitykind:Temperature ;
    s223:hasTag "return",
        "sensor",
        "temperature" ;
    s223:hasUnit unit:DEG_C ;
    s223:isPointOf ex1:RTU-1 .

s223:AC-208VLL-3Ph-60Hz a s223:Medium ;
    s223:hasVoltage [ a s223:VoltageProperty ;
            rdfs:label "AC 208V Line-to-Line 3-Phase 60Hz" ;
            s223:hasVoltage [ a s223:VoltageMagnitude ;
                    rdfs:label "208 Volts AC" ;
                    s223:hasValue 208.0 ;
                    qudt:hasUnit unit:V ] ] .

ex:floor_1 a core:Floor ;
    rdfs:label "First Floor" ;
    core:isPartOfBuilding ex:building_rec ;
    props:hasLevel 1 .

ex:rtu_1 a brick:RTU ;
    rdfs:label "Rooftop HVAC Unit 1" ;
    owl:sameAs ex1:RTU-1 ;
    brick:feeds ex:hvac_zone_1 ;
    brick:hasLocation ex:roof ;
    brick:hasPoint ex:rtu_1_dat,
        ex:rtu_1_fan_cmd ;
    brick:isPartOf ex:building .

ex:rtu_1_dat a brick:Discharge_Air_Temperature_Sensor ;
    rdfs:label "RTU 1 Discharge Air Temperature Sensor" .

ex:rtu_1_fan_cmd a brick:Fan_Command ;
    rdfs:label "RTU 1 Fan Command" .

ex1:RTU-1_Compressor-1 a s223:Compressor ;
    s223:hasDescription "Compressor 1 for RTU-1" ;
    s223:hasTag "compressor" .

ex1:RTU-1_CoolingCoil_ValvePositionSensor a s223:Sensor ;
    s223:hasQuantityKind quantitykind:Dimensionless ;
    s223:hasTag "position",
        "sensor" ;
    s223:hasUnit unit:PERCENT ;
    s223:isPointOf ex1:RTU-1_CoolingCoil .

ex1:RTU-1_EconomizerDamper_PositionSensor a s223:Sensor ;
    s223:hasQuantityKind quantitykind:Dimensionless ;
    s223:hasTag "position",
        "sensor" ;
    s223:hasUnit unit:PERCENT ;
    s223:isPointOf ex1:RTU-1_EconomizerDamper .

ex1:RTU-1_ElectricalInlet a s223:InletConnectionPoint ;
    s223:hasMedium s223:AC-208VLL-3Ph-60Hz ;
    s223:hasTag "electrical",
        "inlet" .

ex1:RTU-1_HeatingCoil_ValvePositionSensor a s223:Sensor ;
    s223:hasQuantityKind quantitykind:Dimensionless ;
    s223:hasTag "position",
        "sensor" ;
    s223:hasUnit unit:PERCENT ;
    s223:isPointOf ex1:RTU-1_HeatingCoil .

ex1:RTU-1_SupplyFan_PowerSensor a s223:Sensor ;
    s223:hasQuantityKind quantitykind:Power ;
    s223:hasTag "power",
        "sensor" ;
    s223:hasUnit unit:W ;
    s223:isPointOf ex1:RTU-1_SupplyFan .

ex1:RTU-1_SupplyFan_SpeedSensor a s223:Sensor ;
    s223:hasQuantityKind quantitykind:Frequency ;
    s223:hasTag "sensor",
        "speed" ;
    s223:hasUnit unit:HZ ;
    s223:isPointOf ex1:RTU-1_SupplyFan .

ex1:RTU-1_SupplyFan_Status a s223:State ;
    s223:hasTag "fan",
        "status" ;
    s223:isPointOf ex1:RTU-1_SupplyFan .

ex1:Zone-1 a s223:Zone ;
    s223:hasDescription "HVAC Zone 1" ;
    s223:hasTag "zone" .

unit:V rdfs:label "V" .

ex:building_rec a core:Building ;
    rdfs:label "Example Office Building (REC)" ;
    props:hasAddress "123 Main St, Anytown, USA" ;
    props:hasGeometry [ a geo:Geometry ;
            geo:asWKT "POLYGON((0 0, 0 100, 100 100, 100 0, 0 0))"^^geo:wktLiteral ] .

ex:hvac_zone_1 a brick:HVAC_Zone ;
    rdfs:label "HVAC Zone 1" ;
    owl:sameAs ex:room_101 ;
    brick:isPartOf ex:building .

ex:roof a brick:Roof ;
    rdfs:label "Building Roof" ;
    brick:isPartOf ex:building .

ex:room_101 a core:Room ;
    rdfs:label "Room 101" ;
    core:isPartOfFloor ex:floor_1 ;
    props:hasArea [ a core:Area ;
            props:hasUnit ref:AreaSquareMeter ;
            props:hasValue 100.0 ] ;
    props:hasGeometry [ a geo:Geometry ;
            geo:asWKT "POLYGON((10 10, 10 20, 20 20, 20 10, 10 10))"^^geo:wktLiteral ] .

ex1:RTU-1_CoolingCoil a s223:CoolingCoil ;
    s223:hasDescription "Cooling Coil for RTU-1" ;
    s223:hasPoint ex1:RTU-1_CoolingCoil_ValvePositionSensor ;
    s223:hasTag "coil",
        "cooling" .

ex1:RTU-1_EconomizerDamper a s223:Damper ;
    s223:hasDescription "Economizer Damper for RTU-1" ;
    s223:hasPoint ex1:RTU-1_EconomizerDamper_PositionSensor ;
    s223:hasTag "damper",
        "economizer" .

ex1:RTU-1_HeatingCoil a s223:HeatingCoil ;
    s223:hasDescription "Heating Coil for RTU-1" ;
    s223:hasPoint ex1:RTU-1_HeatingCoil_ValvePositionSensor ;
    s223:hasTag "coil",
        "heating" .

ex:building a brick:Building ;
    rdfs:label "Example Office Building" ;
    owl:sameAs ex:building_rec ;
    brick:hasPart ex:hvac_zone_1,
        ex:roof,
        ex:rtu_1 .

ex1:RTU-1_SupplyFan a s223:Fan ;
    s223:hasDescription "Supply Fan for RTU-1" ;
    s223:hasPoint ex1:RTU-1_SupplyFan_PowerSensor,
        ex1:RTU-1_SupplyFan_SpeedSensor,
        ex1:RTU-1_SupplyFan_Status ;
    s223:hasTag "fan" .

ex1:RTU-1 a s223:AirHandlingUnit ;
    s223:hasComponent ex1:RTU-1_Compressor-1,
        ex1:RTU-1_CoolingCoil,
        ex1:RTU-1_EconomizerDamper,
        ex1:RTU-1_HeatingCoil,
        ex1:RTU-1_SupplyFan ;
    s223:hasConnectionPoint ex1:RTU-1_ElectricalInlet ;
    s223:hasDescription "Rooftop Unit 1" ;
    s223:hasTag "hvac",
        "rtu" ;
    s223:isLocatedIn ex1:Zone-1 .

